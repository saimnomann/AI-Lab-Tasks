    "Marks=[70,80,90]\n",
    "print(\"Marks :\",end=\" \")\n",
    "for mark in Marks:\n",
    "  print(mark,end=\" \")\n",
    "def calculate_average(marks):\n",
    "  avg=(marks[0]+marks[1]+marks[2])/3\n",
    "  return avg\n",
    "average=calculate_average(Marks)\n",
    "print(\"\\nAverage Marks: \",average)"
   ]
