   "source": [
    "name = input(\"Enter student name: \")\n",
    "marks = input(\"Enter marks: \")\n",
    "marks=int(marks)\n",
    "grade='U'\n",
    "if marks >= 85:\n",
    "  grade = 'A'\n",
    "elif marks >= 70:\n",
    "  grade = 'B'\n",
    "elif marks >= 50:\n",
    "  grade='C' \n",
    "else:\n",
    "  grade='F'\n",
    "\n",
    "print(\"Student Name: \",name,\"\\nMarks: \",marks,\"\\nGrade: \",grade)"
   ]
  }
